<div class="beerCard col-4" style="width: 33.333%;">
    <img class="beer-img" src="{{this.image_url}}" alt=" image cap">
    <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <p class="card-tag">{{this.tagline}}</p>
        <p class="card-text">{{this.description}}</p>
    </div>
</div>